generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id          String @id @default(uuid())  
  password    String
  nick        String @unique
  createdAt   DateTime @default(now())
}
model Primer {
  id          String @id @default(uuid())
  nick        String @unique
  name        String
  description String
  text        String
  createdAt   DateTime @default(now())  
}